{
  "name": "sliding-logs",
  "version": "0.0.1",
  "description": "a simple sliding-logs",
  "author": "Irene La Bollita <irene.labollita@iad2.it>",
  "license": "MIT",
  "main": "./build/src/index.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "debug": "npm run build && npm run serve",
    "serve": "node build/src/index.js",
    "ts-serve": "ts-node src/index.ts",
    "build": "tsc",
    "start": "cross-env DEBUG=\"*,-express:*\" ts-node-dev --rs --debounce 1000 --inspect --pretty --transpile-only  src/index",
    "start-brk": "cross-env DEBUG=\"*,-express:*\" ts-node-dev --rs  --debounce 1000 --inspect-brk --pretty --transpile-only  src/index",
    "test": "jest --coverage --setupFiles dotenv/config",
    "test:watch": "jest --watch --verbose ",
    "upgrade-own-deps": "npm upgrade @ghii/ghii @ghii/http-loader @ghii/package-json-loader @ghii/yaml-loader @iad-os/aemon-oidc-introspect @iad-os/irene-kills"
  },
  "dependencies": {
    "@ghii/ghii": "0.0.8",
    "@ghii/http-loader": "0.0.4",
    "@ghii/package-json-loader": "^1.1.2",
    "@ghii/yaml-loader": "0.0.4",
    "@godaddy/terminus": "^4.11.1",
    "@iad-os/irene-kills": "0.0.2-alpha",
    "axios": "^0.27.2",
    "celebrate": "^15.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "errorhandler": "^1.5.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-pino-logger": "^7.0.0",
    "helmet": "^5.1.1",
    "http-errors": "^2.0.0",
    "http-status": "^1.5.2",
    "joi": "^17.6.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.5.1",
    "nanoid": "^3.3.2",
    "pino-pretty": "^8.1.0",
    "query-string": "^7.1.1",
    "type-fest": "^2.18.0",
    "wait-on": "^6.0.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.13",
    "@types/express-pino-logger": "^4.0.3",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.23",
    "@types/wait-on": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "mongodb-memory-server": "^8.4.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
